<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="kang" class="com.test02.Emp">
		<constructor-arg name="name" value="chan"/>
		<constructor-arg name="salary" value="2500000"/>
	</bean>
	
	<bean id="park" class="com.test02.Emp">
		<property name="name" value="dojun"/>
		<property name="salary" value="3000000"/>
	</bean>
</beans>
